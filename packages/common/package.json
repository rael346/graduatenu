{
  "name": "@graduate/common",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "module/index.js",
  "browser": "lib/index.js",
  "private": true,
  "scripts": {
    "dev": "yarn build:dev -w",
    "build": "yarn build:dev && yarn buid:browser && yarn build:module",
    "build:dev": "babel --root-mode upward -d dist -x '.ts' *.ts",
    "build:browser": "cross-env BABEL_ENV=browser babel --root-mode upward -d lib -x '.ts' *.ts",
    "build:module": "cross-env BABEL_ENV=module babel --root-mode upward -d module -x '.ts' *.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "axios": "^0.26.1",
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-monorepo-cop": "^1.0.2",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "express": "^4.17.3",
    "http-proxy-middleware": "^2.0.3",
    "husky": "^4.2.5",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "cross-env": "^7.0.3"
  }
}
