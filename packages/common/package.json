{
  "name": "@graduate/common",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "module/index.js",
  "browser": "lib/index.js",
  "private": true,
  "scripts": {
    "dev": "yarn build:dev -w",
    "build": "yarn build:dev && yarn buid:browser && yarn build:module",
    "build:dev": "g:babel --root-mode upward -d dist -x '.ts' *.ts",
    "build:browser": "g:cross-env BABEL_ENV=browser g:babel --root-mode upward -d lib -x '.ts' *.ts",
    "build:module": "g:cross-env BABEL_ENV=module g:babel --root-mode upward -d module -x '.ts' *.ts"
  },
  "devDependencies": {
    "axios": "^0.26.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "dataloader": "^1.4.0"
  }
}
